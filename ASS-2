#include <stdio.h>

int main() {
    int a = 5, b = 3;
    int result;

    result = a & b;
    printf("The result of a & b is %d\n", result);

    result = a | b;
    printf("The result of a | b is %d\n", result);

    result = a ^ b;
    printf("The result of a ^ b is %d\n", result);

    return 0;
}
